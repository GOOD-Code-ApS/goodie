{
  "name": "@goodie-ts/cli",
  "version": "0.1.1",
  "description": "CLI for goodie-ts compile-time dependency injection",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/GOOD-Code-ApS/goodie.git",
    "directory": "packages/cli"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=22.13.0"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "goodie": "dist/bin.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist *.tsbuildinfo"
  },
  "dependencies": {
    "@goodie-ts/transformer": "workspace:*",
    "citty": "^0.1.6"
  },
  "devDependencies": {
    "@types/node": "^22.0.0"
  },
  "files": [
    "dist"
  ]
}
